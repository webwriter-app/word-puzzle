{
  "name": "@webwriter/word-puzzles",
  "version": "0.0.1",
  "keywords": [
    "webwriter-widget"
  ],
  "description": "WebWriter Widget for creating crossword puzzles and / or find-the-word puzzles.",
  "license": "MIT",
  "author": "Alexia Weiss",
  "scripts": {
    "prepublishOnly": "npx @webwriter/build",
    "debug": "npx @webwriter/build dev"
  },
  "exports": {
    "./widgets/webwriter-word-puzzles-crossword.*": {
      "source": "./src/widgets/crossword.ts",
      "default": "./dist/widgets/webwriter-word-puzzles-crossword.*"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.19.1",
    "@webwriter/lit": "^2.0.0",
    "bootstrap-icons": "^1.11.3",
    "font-awesome": "^4.7.0",
    "less": "^4.2.1",
    "lit": "^3.2.0",
    "typedoc": "^0.27.6"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "editingConfig": {
    "./widgets/webwriter-word-puzzles": {
      "defining": true,
      "isolating": true
    }
  }
}
