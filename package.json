{
    "name": "@webwriter/word-puzzles",
    "version": "0.9.1",
    "description": "WebWriter Widget for creating crossword puzzles (and in the future, find-the-word puzzles).",
    "keywords": [
        "webwriter-widget"
    ],
    "license": "MIT",
    "author": "Alexia Weiss",
    "scripts": {
        "prepublishOnly": "npx @webwriter/build",
        "debug": "npx @webwriter/build dev"
    },
    "dependencies": {
        "@shoelace-style/shoelace": "^2.19.1",
        "@webwriter/lit": "^2.0.0",
        "bootstrap-icons": "^1.11.3",
        "font-awesome": "^4.7.0",
        "less": "^4.2.1",
        "lit": "^3.2.0",
        "typedoc": "^0.27.6"
    },
    "devDependencies": {
        "typescript": "^5.7.2"
    },
    "exports": {
        "./widgets/webwriter-crossword.*": {
            "source": "./src/widgets/webwriter-crossword.ts",
            "default": "./dist/widgets/webwriter-crossword.*"
        },
        "./package.json": "./package.json"
    },
    "editingConfig": {
        "./widgets/webwriter-crossword": {
            "defining": true,
            "isolating": true
        }
    }
}